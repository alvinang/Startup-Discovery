<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= content_for? :title ? yield(:title) : "Startup Discovery" %></title>
  <meta name="description" content="<%= content_for?(:description) ? yield(:description) : "Startup Discovery - does site traffic matters?" %>">
  
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'http://res.cloudinary.com/dxxqqjrju/image/upload/v1395814727/favicon_2_qerltk.ico' %>

  <!-- Styles -->
  <link href="http://netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet">
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Roboto+Slab:300" %>
  <%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Montserrat" %>

  <!-- Javascripts -->
  <%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js" %>
  <%= javascript_include_tag "application" %>  
  
  <!-- Morris Graphs -->
  <%= yield :company_show %>

</head>